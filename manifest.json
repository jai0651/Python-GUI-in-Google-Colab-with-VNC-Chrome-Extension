{
  "manifest_version": 2,
  "name": "My VNC Extension",
  "version": "1.0",
  "permissions": ["tabs", "http://*/*", "https://*/*", "activeTab"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
